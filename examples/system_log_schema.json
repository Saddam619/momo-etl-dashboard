{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "SystemLog",
  "type": "object",
  "properties": {
    "log_id": { "type": "integer" },
    "transaction_id": { "type": ["integer", "null"] },
    "event_date": { "type": "string", "format": "date-time" },
    "event_type": { "type": "string", "maxLength": 50 },
    "message": { "type": "string" },
    "service_center": { "type": "string", "maxLength": 30 },
    "created_at": { "type": "string", "format": "date-time" }
  },
  "required": ["log_id", "event_date", "event_type", "message"]
}
